{
  "name": "svilborg/laravel-health",
  "description": "Laravel Health Check",
  "keywords": [
    "framework",
    "laravel",
    "health",
    "health-check"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "laravel/framework": "5.*"
  },
  "require-dev": {
    "guzzlehttp/guzzle": "^6.3",
    "symfony/yaml" : "~3.4|~4.0",
    "phpunit/phpunit": "^8.5",
    "orchestra/testbench": "~3.0",
    "phan/phan": "^2.2",
    "mockery/mockery": "^1.2",
    "predis/predis": "^1.1",
    "squizlabs/php_codesniffer": "^3.4",
    "solarium/solarium": "^5.0"
  },
  "suggest": {
    "guzzlehttp/guzzle": "^6.3",
    "symfony/yaml" : "~3.4|~4.0"
  },
  "autoload": {
    "psr-4": {
      "Health\\": "src"
    }
  },
  "autoload-dev": {
     "psr-4": {
         "Tests\\": "tests/"
     }
  }
}
